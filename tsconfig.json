{
  "extends": "astro/tsconfigs/strict",
  "include": [".astro/types.d.ts", "**/*"],
  "exclude": ["dist"],
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/services/*": ["src/services/*"],
      "@/components/*": ["src/components/*"],
      "@/types": ["src/components/ProviderCalendars/types"]
    },
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true
  }
}
